#include <stdio.h>
int main(){
    int A,B,T;
    int i=1;
    scanf("%d", &T);
    while(i<=T){
        scanf("%d %d\n", &A, &B);
        printf("%d\n",A+B);
        
        
        i+=1;
    }

}

#include <stdio.h>
int main(){
    int A,B,T;
    int i=1;
    scanf("%d", &T);
    while(i<=T){
        scanf("%d %d\n", &A, &B);
        printf("Case #%d: %d + %d = %d\n",i,A,B,A+B);
        
        
        i+=1;
    }

}
#include <stdio.h>
int main(){
    int T;
    int i=1;
    scanf("%d", &T);
    while(i<=T){
        for(int a=i; a<T; a+=1){
            printf(" ");
            
        }
        for(int a=1; a<=i; a+=1){
            printf("*");
            
        }
        printf("\n");
        
        i+=1;
    }

}

#include <stdio.h>
int main(){
    int A,B,T,G,H;
    int i=0;
    scanf("%d", &T);
    A=T/10;
    B=T%10;
    G=-1;
    H=-1;
    while(H!=T){
        G=A+B;
        A=G%10;
        H=10*B+A;
        A=B;
        B=G%10;
        i+=1;
    }
    printf("%d",i);

}
